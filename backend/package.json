    "eslint-loader": "4.0.0",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.4",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "ts-node": "^8.10.1"
