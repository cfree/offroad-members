{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:**",
    "start:lambda": "netlify-lambda serve src --config webpack.functions.js",
    "build": "run-p build:**",
    "build:lambda": "netlify-lambda build src --config webpack.functions.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/bundle/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "deploy": "prisma deploy --env-file .env"
  },
  "dependencies": {
    "@apollo/react-components": "^3.1.5",
    "@apollo/react-hooks": "^3.1.5",
    "@sendgrid/mail": "^7.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.13.6",
    "@types/nodemailer": "^6.4.0",
    "apollo-server-express": "^2.13.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.13.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-binding": "^2.5.2",
    "graphql-cli": "^3.0.14",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "nodemon": "^2.0.4",
    "prisma": "^1.34.10",
    "serverless-http": "^2.4.1",
    "stripe": "^8.52.0",
    "typescript": "^3.9.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/parser": "2.28.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.0.4",
    "eslint": "6.8.0",
